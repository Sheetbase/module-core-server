{
   "name": "@sheetbase/server",
   "version": "1.0.0-rc.0",
   "description": "Sheetbase core module for backend app.",
   "author": "Sheetbase",
   "homepage": "https://sheetbase.net",
   "license": "MIT",
   "files": [
      "dist"
   ],
   "main": "dist/bundles/sheetbase-server.umd.js",
   "module": "dist/fesm3/sheetbase-server.js",
   "typings": "dist/sheetbase-server.d.ts",
   "scripts": {
      "module": "sheetbase-app-scripts build --module",
      "readme": "sheetbase-app-scripts readme",
      "docs": "sheetbase-app-scripts docs",
      "build": "npm run module && npm run readme && npm run docs",
      "push": "cd deploy && clasp push",
      "lint": "tslint --project tslint.json && echo 'All files pass linting.'",
      "test": "tsc --project tests/tsconfig.test.json && nyc --cache false mocha --timeout 100000 build/tests/*.js",
      "coverage": "nyc --cache false report --reporter=text-lcov | coveralls"
   },
   "nyc": {
      "exclude": [
         "**/tests"
      ]
   },
   "keywords": [
      "google-apps-script",
      "sheetbase",
      "webapp",
      "api",
      "expressjs"
   ],
   "repository": {
      "type": "git",
      "url": "https://github.com/sheetbase/server.git"
   },
   "bugs": {
      "url": "https://github.com/sheetbase/server/issues"
   },
   "dependencies": {},
   "devDependencies": {
      "@sheetbase/app-scripts": "^1.0.0-rc.0",
      "@types/chai": "^4.1.7",
      "@types/google-apps-script": "^0.0.44",
      "@types/mocha": "^5.2.6",
      "@types/node": "^11.13.4",
      "chai": "^4.2.0",
      "coveralls": "^3.0.3",
      "mocha": "^6.1.3",
      "nyc": "^13.3.0",
      "tslint": "^5.15.0",
      "typescript": "^3.4.3"
   }
}
